{
  "compilerOptions": {
    "rootDir":"dev/app",
    "outDir":"app",
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true
  },
    "exclude": [
        "node_modules",
        "app",
        "wwwroot"
    ]
}
